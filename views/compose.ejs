<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <h4>Title</h4>
    <input type="text" class="form-control" name="postTitle" id="title" autocomplete="off">
    <h4>Post</h4>
    <textarea name="postBody" class="form-control" id="post" cols="30" rows="10"></textarea>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer") -%>
